{
  "name": "@zhinjs/schema",
  "version": "0.0.8",
  "description": "zhin配置声明模块",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "node .",
    "compile": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build": "npm run compile && dtsc --project tsconfig.dtsc.json",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/test.ts",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhinjs/schema.git"
  },
  "keywords": [
    "schema",
    "zhin",
    "zhinjs"
  ],
  "author": "凉菜",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhinjs/schema/issues"
  },
  "homepage": "https://github.com/zhinjs/schema#readme",
  "devDependencies": {
    "@types/node": "latest",
    "dtsc": "^2.3.0",
    "ts-node-dev": "latest",
    "tsc-alias": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/**/LICENSE"
  ],
  "dependencies": {
    "@zhinjs/shared": "^0.0.7"
  }
}
